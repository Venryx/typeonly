{
  "name": "@typeonly/checker-cli",
  "version": "0.3.4",
  "description": "A CLI to validate JSON files, using TypeScript typing definitions.",
  "author": "Paroi",
  "scripts": {
    "prepublishOnly": "npm run build",
    "clear": "rimraf 'dist/*'",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "build": "npm run clear && npm run tsc",
    "lint": "tslint -p tsconfig.json -t verbose"
  },
  "dependencies": {
    "@typeonly/checker": "^0.3.4",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "typeonly": "^0.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/node": "^14.0.12",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "bin": "dist/cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paroi-tech/typeonly.git"
  },
  "homepage": "https://github.com/paroi-tech/typeonly/tree/master/checker-cli",
  "license": "CC0-1.0",
  "keywords": [
    "typescript",
    "json",
    "validation"
  ]
}
