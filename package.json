{
  "name": "typeonly",
  "version": "0.2.3",
  "description": "Parses typing definitions from TypeScript and brings them at runtime.",
  "author": "tomko.team",
  "scripts": {
    "antlr": "java -cp \"antlr-4.7.2-complete.jar\" org.antlr.v4.Tool -Dlanguage=JavaScript -o antlr-parser TypeOnlyParser.g4 TypeOnlyLexer.g4",
    "clear": "rimraf 'dist/*' 'antlr-parser/*' 'bundle-tsd/declarations/*'",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "bundle-tsd": "node bundle-tsd/bundle-tsd",
    "build": "npm run clear && npm run antlr && npm run tsc && npm run bundle-tsd",
    "lint": "tslint -p tsconfig.json -t verbose",
    "test:watch": "jest --watch",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "antlr4": "^4.7.2",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.7",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "main": "dist/api.js",
  "types": "dist/typeonly.d.ts",
  "bin": "./dist/cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomko-team/typeonly.git"
  },
  "license": "CC0-1.0",
  "keywords": [
    "typescript",
    "types",
    "runtime"
  ]
}