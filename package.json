{
  "name": "@typeonly/reader",
  "version": "0.1.3",
  "description": "Brings TypeOnly metadata at run time.",
  "author": "tomko.team",
  "scripts": {
    "clear": "rimraf 'dist/*' 'bundle-tsd/declarations/*'",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "bundle-tsd": "node bundle-tsd/bundle-tsd",
    "build": "npm run clear && npm run tsc && npm run bundle-tsd",
    "lint": "tslint -p tsconfig.json -t verbose",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^10.14.8",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typeonly": "^0.1.4",
    "typescript": "^3.5.1"
  },
  "main": "dist/api.js",
  "types": "dist/reader.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomko-team/typeonly-reader.git"
  },
  "license": "CC0-1.0",
  "keywords": [
    "typing",
    "runtime",
    "interface",
    "type",
    "typescript"
  ]
}