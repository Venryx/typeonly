{
  "name": "@tomko/typeonly",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "tomko.team",
  "scripts": {
    "antlr": "java -cp \"antlr-4.7.2-complete.jar\" org.antlr.v4.Tool -Dlanguage=JavaScript TypeOnlyLexer.g4 TypeOnlyParser.g4 -o antlr-parser",
    "clear": "rimraf 'dist/*' 'antlr-parser/*'",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "build": "npm run clear && npm run antlr && npm run tsc",
    "lint": "tslint -p tsconfig.json -t verbose",
    "test": "jest --watch"
  },
  "dependencies": {
    "antlr4": "^4.7.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}