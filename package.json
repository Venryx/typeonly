{
  "name": "@typeonly/checker-cli",
  "version": "0.2.2",
  "description": "A CLI to validate JSON files, using TypeScript typing definitions.",
  "author": "tomko.team",
  "scripts": {
    "clear": "rimraf 'dist/*'",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "build": "npm run clear && npm run tsc",
    "lint": "tslint -p tsconfig.json -t verbose",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@typeonly/checker": "^0.2.2",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "typeonly": "^0.2.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/node": "^10.17.3",
    "rimraf": "^3.0.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "bin": "dist/cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomko-team/typeonly-checker-cli.git"
  },
  "license": "CC0-1.0",
  "keywords": [
    "typescript",
    "json",
    "validation"
  ]
}